#include<stdio.h>
int sumArray(int *arr,int n)
{
    int sum=0;
    int *ptr=arr;
    while(ptr<arr+n)
    {
        sum=sum+*ptr;
        ptr++;
    }
    return sum;
}
    int main () 
    {
        int a[5];
        printf("enter the aray elements\n");
        for(int i=0;i<5;i++)
        {
            scanf("%d",&a[i]);
        }
        int total=sumArray(a,5);
        printf("%d",total);
        return 0;
    }
